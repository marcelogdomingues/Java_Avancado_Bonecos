<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	      http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	      version="2.5">

	<servlet>
		<servlet-name>Registo</servlet-name>
		<servlet-class>com.bonecos.kuscos.servlet.Registo</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Registo</servlet-name>
		<url-pattern>/servlet/Registo</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>criar.html</welcome-file>
	</welcome-file-list>

</web-app>